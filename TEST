public static final Item GARNET_CHESTPLSATE = registerItem("garnet_chestplate",
            new ArmorItem(ModArmorMaterials.GARNET, EquipmentSlot.HEAD,
                    new FabricItemSettings().group(ModItemGroup.MOD)));

